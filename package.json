  "bugs": {
    "url": "https://github.com/yondonfu/opencollab/issues"
  },
  "homepage": "https://github.com/yondonfu/opencollab#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-register": "^6.24.1",
    "fs-extra": "^2.1.2",
    "mkdirp": "^0.5.1",
    "shelljs": "^0.7.7",
    "swarm-js": "^0.1.10",
    "truffle-contract": "^2.0.0",
    "web3": "^0.16.0",
    "yargs": "^4.7.1"
